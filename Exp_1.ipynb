        "def finddis(s):\n",
        " x=y=0\n",
        " for i in s:\n",
        "  if(i=='U'):\n",
        "   x+=1\n",
        "  elif(i=='D'):\n",
        "   x-=1\n",
        "  elif(i=='L'):\n",
        "   y-=1\n",
        "  elif(i=='R'):\n",
        "   y+=1\n",
        " return x,y\n",
        "s='UDLRUDLRDL'\n",
        "x1,y1=finddis(s)\n",
        "print(\"Final Position: ( {} , {} )\".format(x1,y1))"
      ]
    }
  ]
